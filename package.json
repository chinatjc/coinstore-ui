{
  "name": "@coinstore-ui/base",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint-all": "eslint . --ignore-pattern dist --ignore-pattern coverage --ignore-pattern .rollup.cache --report-unused-disable-directives --max-warnings 0",
    "lint": "eslint --ignore-pattern dist --ignore-pattern coverage --ignore-pattern .rollup.cache --report-unused-disable-directives --max-warnings 0",
    "tsc-all": "tsc --noEmit",
    "prettier-check-all": "prettier . -c",
    "prettier-check": "prettier -c",
    "prettier-fix-all": "prettier . --ignore-unknown --write",
    "prettier-fix": "prettier --ignore-unknown --write",
    "build-es": "rollup --config rollup.config.ts --configPlugin typescript --environment TARGET:all",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "test": "jest --bail --passWithNoTests",
    "test-all": "jest --bail",
    "prepublishOnly": "npm run prettier-check-all && npm run lint-all && npm run tsc-all && npm run test-all"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@chromatic-com/storybook": "^1.5.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/prompt-cli": "^19.3.1",
    "@commitlint/types": "^19.0.3",
    "@eslint/js": "^9.1.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.1.9",
    "@storybook/addon-interactions": "^8.1.9",
    "@storybook/addon-links": "^8.1.9",
    "@storybook/addon-onboarding": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/manager-api": "^8.2.6",
    "@storybook/react": "^8.1.9",
    "@storybook/react-vite": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@storybook/theming": "^8.2.6",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.12.8",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "glob": "^10.4.5",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": ">=9.2.15 <9.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.40",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.76.0",
    "storybook": "^8.1.9",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1",
    "vite": "^5.2.0"
  },
  "engines": {
    "node": ">=21.1.0",
    "npm": ">=10.2.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
